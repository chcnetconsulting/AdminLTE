{
    "name": "chcnet/AdminLTE",
    "description": "AdminLTE 2.0 Theme plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "AdminLTE\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdminLTE\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
